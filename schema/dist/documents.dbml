// === TABLE DEPENDENCY: projects ===
Table projects {
  id int [pk, increment]
  nama varchar [not null]
  deskripsi text
  tanggal_mulai date
  tanggal_selesai date
}

// === ENTITY: documents ===
Table documents {
    id int [pk, increment]
    project_id int [ref: > projects.id]
    nama varchar
    kode varchar
}

// === DIRECT RELATION: /entities/user_stories.dbml ===
Table user_stories {
  dokumen_id int [ref: > documents.id]
}
