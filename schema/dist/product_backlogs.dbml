// === TABLE DEPENDENCY: statuses ===
Table statuses {
  id int [pk, increment]
  nama varchar
  deskripsi varchar
}

// === TABLE DEPENDENCY: squads ===
Table squads {
  id int [pk, increment]
  nama varchar [not null]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === ENTITY: product_backlogs ===
Enum prioritas {
  LOW
  MEDIUM
  HIGH
}

Table product_backlogs {
  id int [pk, increment]
  squad_id int [ref: > squads.id]
  status_id int [ref: > statuses.id]
  judul varchar(255)
  prioritas prioritas
}

