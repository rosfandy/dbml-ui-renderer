// === ENTITY: projects ===
Table projects {
  id int [pk, increment]
  nama varchar [not null]
  deskripsi text
  tanggal_mulai date
  tanggal_selesai date
}

// === DIRECT RELATION: /entities/attachments.dbml ===
Table attachments {
  project_id int [ref: > projects.id]
}

// === DIRECT RELATION: /entities/documents.dbml ===
Table documents {
  project_id int [ref: > projects.id]
}

// === DIRECT RELATION: /entities/scrum/scrum_sprints.dbml ===
Table scrum_sprints {
  project_id int [ref: > projects.id]
}

// === DIRECT RELATION: /entities/scrum/scrum_tasks.dbml ===
Table scrum_tasks {
  project_id int [ref: > projects.id]
}

// === DIRECT RELATION: /entities/squads.dbml ===
Table squads {
  project_id int [ref: > projects.id]
}

// === DIRECT RELATION: /entities/user_stories.dbml ===
Table user_stories {
  project_id int [ref: > projects.id]
}
