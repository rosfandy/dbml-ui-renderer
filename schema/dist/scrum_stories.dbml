// === TABLE DEPENDENCY: projects ===
Table projects {
  id int [pk, increment]
  nama varchar [not null]
  deskripsi text
  tanggal_mulai date
  tanggal_selesai date
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: dokumens ===
Table dokumens {
  id int [pk, increment]
  project_id int [ref: - projects.id]
  nama varchar
  kode varchar
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === ENTITY: scrum_stories ===
Table scrum_stories {
  id int [pk, increment]
  dokumen_id int [ref: > dokumens.id]
  project_id int [ref: > projects.id]
  nama varchar 
  kode varchar
}
