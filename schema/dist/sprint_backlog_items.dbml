// === TABLE DEPENDENCY: statuses ===
Table statuses {
  id int [pk, increment]
  name varchar
  position_id int [ref: > positions.id]
  index int
  color varchar
  category status
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
} 

// === TABLE DEPENDENCY: products ===
Table products {
  id int [pk, increment]
  name varchar
  project_id int [ref: > projects.id]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: epics ===
Table epics {
    id int [pk, increment]
    name varchar [not null]
    product_id int [ref: > products.id]
    created_at timestamp [default: `CURRENT_TIMESTAMP`]
    updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: roles ===
Table roles {
  id int [pk, increment]
  name varchar
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: levels ===
Table levels {
  id int [pk, increment]
  name varchar [not null]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: positions ===
Table positions {
    id int [pk, increment]
    name varchar [not null]
    department_id int [ref: > departments.id]
    created_at timestamp [default: `CURRENT_TIMESTAMP`]
    updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: departments ===
Table departments {
  id int [pk, increment]
  name varchar [not null]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: titles ===
Table titles {
  id int [pk, increment]
  name varchar [not null]
  department_id int [ref: > departments.id]
  position_id int [ref: > positions.id]
  level_id int [ref: > levels.id]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: users ===
Table users {
  id int [pk, increment]
  title_id int [ref: - titles.id]
  role_id int [ref: > roles.id]
  name varchar [not null]
  email varchar [not null, unique]
  email_verified_at timestamp
  password varchar [not null]
  remember_token varchar
  custom_fields json
  avatar_url varchar
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: backlogs ===
Table backlogs {
  id int [pk, increment]
  pic int [ref: > users.id]
  project_id int [ref: > projects.id]
  epic_id int [ref: > epics.id]
  code varchar 
  priority prioritas
  description text
  start_date date
  end_date date
  created_by int [ref: > users.id]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: backlog_items ===
Table backlog_items {
  id int [pk, increment]
  backlog_id int [ref: > backlogs.id]
  assigned_to int [ref: > users.id]
  status_id int [ref: > statuses.id]
  estimate int
  code varchar
  start_date date
  end_date date
  priority prioritas
  description text
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: projects ===
Table projects {
  id int [pk, increment]
  name varchar [not null]
  code varchar
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: squads ===
Table squads {
  id int [pk, increment]
  project_id int [ref: > projects.id]
  name varchar
  code varchar
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

// === TABLE DEPENDENCY: sprints ===
Table sprints {
  id int [pk, increment]
  name varchar
  code varchar
  total_estimate int
  start_date datetime
  end_date datetime
  squad_id int [ref: > squads.id]
  created_at timestamp [default: `CURRENT_TIMESTAMP`]
  updated_at timestamp [default: `CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP`]
}

Table sprint_backlog_items {
    id int [pk, increment]
    sprint_id int [ref: > sprints.id]
    backlog_item_id int [ref: > backlog_items.id]
}